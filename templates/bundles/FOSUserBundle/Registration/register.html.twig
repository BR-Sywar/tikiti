<script>

  var placesAutocomplete = places({
    appId: 'plJ9KMPU6SCC',
    apiKey: 'c8c402900b72a30b50b2f803fddee305',
    container: document.querySelector('#fos_user_registration_form_adresse')
  });

  placesAutocomplete.on('change', function resultSelected(e) {
      document.getElementById('fos_user_registration_form_ville').value = e.suggestion.name;
      document.getElementById('fos_user_registration_form_lat').value = e.suggestion.latlng.lat;
      document.getElementById('fos_user_registration_form_lng').value = e.suggestion.latlng.lng;
      document.getElementById('fos_user_registration_form_codePostale').value = e.suggestion.postcode;

  });

</script>

