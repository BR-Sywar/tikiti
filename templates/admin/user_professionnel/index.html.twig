{% extends 'admin/admin.html.twig' %}

{% block body %}

<div class="row mt">
          <div class="col-lg-12">
            <h4><i class="fa fa-angle-right"></i> Liste des utilisateurs professionnel</h4>
            <div class="form-panel">
              <div class="table-responsive-xl">
  <table class="table">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Prénom</th>
            <th>Email</th>
            <th>Téléphone</th>
            <th>Logo</th>
        </tr>
    </thead>

    <tbody>
        {% for userPro in user_professionnels %}
            <tr>
                <td>userPro.nom</td>
                <td>userPro.prenom</td>
                <td>userPro.email</td>
                <td>userPro.tel</td>
                <td>userPro.urlLogo</td>
        {% endfor %}
    </tbody>
    
</table>
</div>
            </div>
            <!-- /form-panel -->
          </div>
          <!-- /col-lg-12 -->
        </div>



       
{% endblock %}