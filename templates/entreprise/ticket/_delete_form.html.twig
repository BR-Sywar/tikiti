<form method="post" action="{{ path('ticket_delete_front', {'id': ticket.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
     <div style="font-size: large;">
     <center>
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ticket.id) }}">
    button class="btn btn-danger fa fa-trash-o"></button></center>
    </div>
</form>

